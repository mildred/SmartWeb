# curl -v -X POST --data-binary @openrdf-sesame-smartweb.trig -H "Content-Type: application/x-trig;charset=UTF-8" http://localhost:8080/repositories/SYSTEM/statements
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix rep: <http://www.openrdf.org/config/repository#>.
@prefix sr: <http://www.openrdf.org/config/repository/sail#>.
@prefix sail: <http://www.openrdf.org/config/sail#>.
@prefix sys: <http://www.openrdf.org/config/repository#> .
@prefix ns: <http://www.openrdf.org/config/sail/native#>.

_:node001 {
 [] a rep:Repository ;
   rep:repositoryID "smartweb" ;
   rdfs:label "SmartWeb Store" ;
   rep:repositoryImpl [
      rep:repositoryType "openrdf:SailRepository" ;
      sr:sailImpl [
         sail:sailType "openrdf:NativeStore" ;
         ns:tripleIndexes "spoc,posc"
      ]
   ].
}
{
  _:node001 a sys:RepositoryContext .
}